public static Integer maxSubArraySum(List<Integer> nums, Integer target){
    Integer total = 0;
    Integer k = 0;
    for(Integer i = 0 ; i < target ; i++){
        total+=nums[i];   
    }
    Integer temp  = total;
    for(Integer i = target + k ; i < nums.size(); k++){
        temp = total - nums[k] + nums[i] ;
        total = total > temp ? total : temp
    }
    return total;
}