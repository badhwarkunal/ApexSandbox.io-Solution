public static Integer maxSubArraySum(List<Integer> nums, Integer target){
    Integer total = 0;
    Integer k = 0;
    for(Integer i = 0 ; i < target ; i++){
        total+=nums[i];   
    }
    for(Integer i = target ; i < nums.size(); i++){
        if(total > total - nums[k] + nums[i]){

        }
        total = total > total - nums[k] + nums[i] ? total : total - nums[k] + nums[i];
    }
    return total;
}