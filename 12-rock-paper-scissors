public Integer rockPaperScissors(String player1, String player2) {
    //code here
    Map<String,String> wc = new Map<String,String>{
        'rock' => 'scissors',
        'paper' => 'rock',
        'scissors' => 'paper'
    };
    if(player1 == player2)
    {
        return 0;
    }
    else if(wc.get(player1)==player2){
        return 1;
    }
    else{
        return 2;
    }
}