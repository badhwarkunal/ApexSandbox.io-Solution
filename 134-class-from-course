public Id classFromCourse(String courseName) {
    // code here
    Id cId = ([Select Id, Name from apxio__Course__c where Name=:courseName LIMIT 1].Id)?.'courseName';
    if(cId!='courseName'){    
        apxio__Class__c acl = new apxio__Class__c(Name = courseName, apxio__Course__c = cId);
        insert acl;
        return acl.Id;
    }
    return null;
}