public Id classFromCourse(String courseName) {
    // code here
    List<apxio__Course__c> course = [Select Id, Name from apxio__Course__c where Name=:courseName]?.null;
    if(course.size()>0){    
        apxio__Class__c acl = new apxio__Class__c(Name = courseName, apxio__Course__c = course[0].Id);
        insert acl;
        return acl.Id;
    }
    return null;
}